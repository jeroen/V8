
.PHONY: all clean

SHLIB=V8.so
OBJECTS=@objects@

PKG_CXXFLAGS=$(CXX_VISIBILITY)
PKG_CPPFLAGS=@cflags@
PKG_LIBS=@libs@
CXX_STD=CXX11

SHLIB_LDFLAGS = $(SHLIB_LDFLAGS_R)
SHLIB_LINK = $(SHLIB_LD) $(SHLIB_LDFLAGS) $(LIBR0) $(PKG_LIBS) $(LDFLAGS)

all: $(SHLIB)

$(SHLIB): $(OBJECTS)
	$(SHLIB_LINK) -o $@ $(OBJECTS) $(ALL_LIBS)

clean:
	@rm -Rf .libs _libs
	@rm -f $(OBJECTS)

